{
  "diagnostic.checkCurrentLine": true,
  "flutter.sdk.path": "/home/cathood/development/flutter/bin/flutter",
  "flutter.sdk.searchPaths": ["/home/cathood/development/flutter/bin"],
  "coc.preferences.enableMarkdown": true,
  "diagnostic.enable": true,
  "diagnostic.autoRefresh": true,
  "diagnostic.showDeprecated": true,
  "suggest.snippetsSupport": true,
  "suggest.enablePreselect": true,
  "suggest.snippetIndicator": " ðŸ’¡",
  "snippets.ultisnips.directories": ["~/.config/nvim/ultisnips"],
  "dart.enableSdkFormatter": true,
  "dart.lineLength": 125,
  "dart.showTodos": true,
  "dart.analysisExcludedFolders": [],
  "dart.completeFunctionCalls": true,
  "flutter.autoOpenDevLog": true,
  "flutter.autoHideDevLog": true,
  "coc.preferences.formatOnSave": false,
  "eslint.autoFixOnSave": false,
  "prettier.requireConfig": false,
  "prettier.disableLanguages": ["vue"],
  "prettier.tabWidth": 2,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "eslint.packageManager": "yarn",
  "eslint.nodePath": ".yarn/sdks",
  "workspace.workspaceFolderCheckCwd": false,
  "tsserver.tsdk": ".yarn/sdks/typescript/lib",
  "languageserver": {
    "html": {
      "command": "vscode-html-languageserver",
      "args": ["--stdio"],
      "filetypes": ["html"]
    },
    "css": {
      "command": "vscode-css-languageserver",
      "args": ["--stdio"],
      "filetypes": ["css", "scss"]
    },
    "javascript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["javascript", "javascriptreact"]
    },
    "typescript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "filetypes": ["typescript", "typescriptreact"]
    },
    "python": {
      "command": "pyls",
      "filetypes": ["python"],
      "settings": {
        "pyls": {
          "configurationSources": ["flake8"],
          "plugins": {
            "pyls_mypy": {
              "enabled": true,
              "live_mode": false
            },
            "pyls_black": {
              "enabled": true
            },
            "pyls_isort": {
              "enabled": true
            },
            "pyls_flake8": {
              "enabled": true
            }
          }
        }
      }
    }
  }
}
